{
  "expo": {
    "name": "OJTSTUDENTSYSTEM",
    "slug": "OJTSTUDENTSYSTEM",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBUXVNafdTkfBAUI6DX_WmKrqr1xtHbSoA"
      },
      "infoPlist": {
